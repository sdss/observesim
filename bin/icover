#!/bin/sh
# icover NPTS  or  icover i,j
if test ! -z "$2"
	then
	case "$1" in
	3)
		shift
		;;
	*)
		echo icover 3 NPTS   or  icover 3 i,j >&2
		exit 1
		;;
	esac
fi
N=$1
if test -z "$N"
	then echo icover NPTS >&2
	exit 1
fi
sed  -n "
/^3 $N /,/^\$/p
" $OBSERVESIM_DIR/cextern/Sloane/codes.icover | grep -v ',' | $OBSERVESIM_DIR/cextern/Sloane/creconstruct
